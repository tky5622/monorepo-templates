    "start": "nx serve",
    "build": "nx build",
    "dev": "nx serve front",
    "test": "nx test",
    "fc:new": "hygen fc new",
    "hooks:new": "hygen hooks new",
    "prepare": "husky install",
    "check-types": "bash -c tsc --noEmit",
    "lint": "nx format:write --all",
    "affected:lint": "nx affected:lint",
    "format:write": "nx format:write",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,tsx,html}": [
      "npm run affected:lint --uncommitted --parallel --fix=true",
      "npm run format:write --uncommitted",
      "npx check-types",
      "git add"
    ]
